import random
karakter = "+-/*!&$#?=@abcdefghijklnopqrstuvwxyzABCDEFGHIJKLMNOPQRSTUVWXYZ1234567890"
sifre_uzunlugu=int(input("şifre uzunluğu kaç olsun"))

sifre=""

for i in range("sifre uzunluğu"):
     sifre=random.choice(karakter) + sifre
     #sifre = random.choice(karakter)

print("sifrem:", sifre)
